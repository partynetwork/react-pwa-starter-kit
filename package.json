  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-2",
      "next/babel"
    ],
    "plugins": [
      [
        "react-intl"
      ],
      [
        "module-resolver",
        {
          "root": [
            "./"
          ]
        }
      ],
      [
        "styled-components",
        {
          "ssr": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node ./server",
    "test": "jest --config ./jest.config.js",
    "lint-js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"src/**/*.{css,less,scss,sss}\""
